  "version": "1.0.0",
  "description": "Practice your english skills",
  "main": "server-side/index.js",
  "scripts": {
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "server": "cd server-side && npm run dev",
      "client": "cd client-side && npm run dev",
      "build": "cd client-side && npm run build",
      "install-client": "cd client-side && npm install",
      "install-server": "cd server-side && npm install",
      "install-all": "npm install && npm run install-client && npm run install-server"
  },
